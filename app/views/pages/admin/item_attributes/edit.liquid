---
layout: admin
slug: admin/item_attributes/:id/edit
---
{% liquid
  function current_profile = 'lib/current_profile', user_id: context.current_user.id
  include 'lib/profile_present_or_redirect', profile: current_profile
  include 'lib/profile_must_be', profile: current_profile, action: 'be_admin'

  function object = 'lib/queries/item_attributes/find_attributes_for_name', name: context.params.id
  function available_languages = 'lib/queries/constants/find', name: "AVAILABLE_LANGUAGES", type: "array"

  include 'theme/simple/admin/item_attributes/edit', object: object, available_languages: available_languages
%}
