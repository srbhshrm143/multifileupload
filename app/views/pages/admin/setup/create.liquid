---
slug: admin/setup
method: post
---
{% liquid
  function current_profile = 'lib/current_profile', user_id: context.current_user.id
  include 'lib/profile_must_be', profile: current_profile, action: 'be_superadmin'
  function object = 'lib/commands/marketplace_setup/create', constants: context.params.constants, context: context
  if object.valid
    redirect_to '/'
  else
    include 'theme/simple/admin/setup/index', object: object
  endif
%}
