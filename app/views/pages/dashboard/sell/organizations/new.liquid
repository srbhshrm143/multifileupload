{% liquid
  function current_profile = 'lib/current_organization_profile', user_id: context.current_user.id
  include 'lib/profile_present_or_redirect', profile: current_profile

  if current_profile.type == 'organization'
    include 'redirect_to', url: '/dashboard/sell/items', notice: 'app.organizations.already_have_organization'
  endif


  include 'theme/simple/dashboard/organizations/new'
%}
