{% liquid
  function relation = 'lib/queries/relationships/find', name: object.name, l_id: object.l_id, r_id: object.r_id
  if relation
    return relation
  endif

  function relation = 'lib/commands/relationships/create', name: object.name, l_id: object.l_id, r_id: object.r_id, l_at: object.l_accepted_at, r_at: object.r_accepted_at, created_by_id: profile.id
  if relation.valid
    hash_assign relation['created'] = true
  endif

  return relation
%}
