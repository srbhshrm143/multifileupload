{% comment %}
  @params
    uuid
{% endcomment %}

{% liquid
  function object = 'lib/commands/activities/publish/build', uuid: uuid, type: type, ids: ids
  function object = 'lib/commands/activities/publish/check', object: object

  if object.valid
    function object = 'lib/commands/activities/publish/execute', object: object
  else
    log object, type: 'Failed publishing event'
  endif
  return object
%}
