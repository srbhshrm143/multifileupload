{% comment %}
  @params
    record_id
{% endcomment %}

{% liquid
  function object = 'lib/commands/activities/unpublish/build', record_id: record_id
  function object = 'lib/commands/activities/unpublish/check', object: object

  if object.valid
    function object = 'lib/commands/activities/unpublish/execute', object: object
  else
    log object, type: 'Failed unpublishing event'
  endif
  return object
%}
