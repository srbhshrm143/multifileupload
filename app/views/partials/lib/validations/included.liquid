{% comment %}
  params: @object
          @field_name
          @c
          @array
          @key[optional]
{% endcomment %}

{% liquid
  unless array contains object[field_name]
    assign key = key | default: "app.errors.not_included"
    function c = 'lib/register_error', contract: c, field_name: field_name, key: key
  endunless
  return c
%}

