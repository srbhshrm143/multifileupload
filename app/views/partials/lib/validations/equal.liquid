{% comment %}
  params: @given
          @expected
          @field_name
          @c
{% endcomment %}
{% liquid
  if given != expected

    if not_verbose
      assign msg = 'test.should.equal_not_verbose' | t
    else
      assign msg = 'test.should.equal' | t: given: given, expected: expected
    endif
    function c = 'lib/register_error', contract: c, field_name: field_name, message: msg
  endif
  return c
%}
