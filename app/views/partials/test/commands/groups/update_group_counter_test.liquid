{% liquid
  function profile = 'test/fixtures/profile', user_id: "1"
  function group = 'test/fixtures/group', owner_id: profile.id, contract: contract

  assign object = '{}' | parse_json
  hash_assign object['id'] = group.id
  hash_assign object['c__members_count'] = 1
  hash_assign object['c__posts_count'] = 1
  function object = 'lib/commands/groups/update_count_cache', object: object

  function contract = 'test/assertions/valid_object', contract: contract, object: object, field_name: 'object'
%}
