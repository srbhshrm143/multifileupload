<h1>Hello User</h1>
<!-- {{ context.params }}<br><br> -->
{{ context }}
{%liquid
  assign fname = context.params.owner_fname
  assign lname = context.params.owner_lname
  assign gender = context.params.owner_gender
  assign mobile_no = context.params.owner_mobile_no
  assign email = context.params.owner_email_id
  assign password = context.params.owner_password

%}
<table>
  <tr>
    <td>{{ "First Name" }}</td>
    <td>{{ fname }}</td>
  </tr>
  <tr>
    <td>{{ "Last Name" }}</td>
    <td>{{ lname }}</td>
  </tr>
  <tr>
    <td>{{ "Gender" }}</td>
    <td>{{ gender }}</td>
  </tr>
  <tr>
    <td>{{ "Mobile" }}</td>
    <td>{{ mobile_no }}</td>
  </tr>
  <tr>
    <td>{{ "Email" }}</td>
    <td>{{ email }}</td>
  </tr>
  <tr>
    <td>{{ "Password" }}</td>
    <td>{{ password }}</td>
  </tr>
</table>
<!-- {{ "First Name" }} - {{ fname }}<br>
{{ "Last Name" }} - {{ lname }}<br>
{{ "Gender" }} - {{ gender }}<br>
{{ "Mobile" }} - {{ mobile_no }}<br>
{{ "Email" }} - {{ email }}<br>
{{ "Password" }} - {{ password }}<br> -->


{%- comment -%}
{%liquid graphql create_gql = 'venue_owner/create', owner_fname: fname, owner_lname: lname, owner_gender: gender, owner_mobile: mobile_no, owner_email: email, owner_password: password
  assign data = create_gql | fetch: "model_create"
%}
{%- endcomment -%}

