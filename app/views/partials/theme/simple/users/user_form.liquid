        </fieldset> -->
        
        <input type="radio" name="tabs_tabs" id="tab__1" class="tab__radio" checked="checked">
        <label for="tab__1" class="font-semibold"> Event Space- </label>
  
        <fieldset class="w-full tab__content">
  
          <table id="my_table_data" class="w--90">
            <tr class="event_space w-full">
              <th class="event_space_features"> {{ "Features." }} </th>
              <th class="event_space_base_price tab_field_price"> {{ "Included in Base Price." }} </th>
              <th class="event_space_na tab_field_na"> {{ "NA" }} </th>
              <th class="event_space_flat_rate tab_field_flat_rate"> {{ "Flat Rate" }} </th>
              <th class="event_space_guest_price tab_field_guest_price"> {{ "Price Per Guest" }} </th>
              <th class="event_space_capacity tab_field_capacity"> {{ "Capacity" }} </th>
            </tr>
  
            <tr class="event_space w-full">
