{
  "scripts": {
    "build": "npx cross-env NODE_ENV=production webpack-cli",
    "build:dev": "npx cross-env NODE_ENV=development webpack-cli",
    "ci:build": "docker run -it -u $(id -u ${USER}):$(id -g ${USER}) --rm -v $PWD:/app -w /app node:12-alpine npm ci && npm run build",
    "ci:test": "docker run -it -e MPKIT_URL=$MPKIT_URL -u $(id -u ${USER}):$(id -g ${USER}) --rm -v $PWD:/app -w /app platformos/testcafe \"npm run test\"",
    "ci:test:publish": "sh scripts/upload-test-report.sh",
    "ci:test:unit": "sh scripts/run-unit-tests.sh",
    "clean": "npx rimraf app/assets/{js,css}/*",
    "deploy": "npm run build && CI=true pos-cli deploy",
    "deploy:dev": "npm run build:dev && pos-cli deploy dev",
    "dev-gui": "pos-cli gui serve dev",
    "dev-sync": "pos-cli sync dev --livereload",
    "dev-watch": "npx cross-env NODE_ENV=development webpack-cli -w",
    "postinstall": "npm i --prefix scaffold",
    "predeploy": "npm run clean",
    "start": "npx concurrently --kill-others-on-fail npm:dev-*",
    "test-concurrently": "testcafe 'chromium:headless --sandbox' test --reporter html:test-report.html -s path=test/screenshots,fullPage=true,takeOnFails=true --selector-timeout 10000 -c 3 --fixture-meta concurrency=true",
    "test": "testcafe 'chromium:headless --sandbox' test/community_tests.js test/projects_tests.js --reporter html:test-report.html -s path=test/screenshots,fullPage=true,takeOnFails=true --selector-timeout 10000 --fixture-meta concurrency=false",
    "smoketest": "testcafe 'chromium:headless --sandbox' --stop-on-first-fail smoketests --reporter html:test-report.html -s path=test/screenshots,fullPage=true,takeOnFails=true --selector-timeout 10000",
    "test:codecept": "npx codeceptjs run --reporter mochawesome",
    "test:mp:chrome": "MPKIT_URL=${MPKIT_URL} testcafe 'chrome --start-maximized'",
    "eslint": "eslint src/js --ext .js"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/typography": "^0.4.1",
    "autoprefixer": "^10.3.6",
    "concurrently": "^6.2.1",
    "css-loader": "^6.3.0",
    "esbuild-loader": "^2.15.1",
    "eslint": "^8.0.0",
    "mini-css-extract-plugin": "^2.3.0",
    "postcss": "^8.3.8",
    "postcss-fixes": "^3.0.0",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.1.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.2.16",
    "tailwindcss-rtl": "^0.7.3",
    "webpack": "^5.54.0",
    "webpack-cli": "^4.8.0",
    "webpack-require-from": "^1.8.6"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  },
  "browserslist": "> 1% or last 2 years",
  "name": "marketplace-template-poc",
  "version": "1.0.0",
  "description": "eCommerce MVP POC",
  "author": "platformOS",
  "license": "ISC",
  "repository": "mdyd-dev/marketplace-template-poc",
  "dependencies": {
    "@uppy/aws-s3": "^2.0.3",
    "@uppy/core": "^2.0.3",
    "@uppy/dashboard": "^2.0.3",
    "@uppy/drag-drop": "^2.0.2",
    "@uppy/webcam": "^2.0.2",
    "@yaireo/tagify": "^4.8.1",
    "alpinejs": "^3.4.1",
    "chokidar": "^3.5.3",
    "easymde": "^2.15.0",
    "faker": "^5.5.3",
    "highlight.js": "^11.2.0",
    "lodash.debounce": "^4.0.8",
    "lodash.groupby": "^4.6.0",
    "lodash.isequal": "^4.5.0",
    "lodash.uniqby": "^4.7.0",
    "timeago.js": "^4.0.2",
    "uuid": "^8.3.2"
  }
}
